
<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to='ifgov'>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. -->
<!-- 	<inherits name='com.google.gwt.user.theme.standard.Standard' /> -->
<!-- 	<inherits name="com.google.gwt.user.theme.dark.Dark"/> -->
	<inherits name="com.google.gwt.user.theme.chrome.Chrome"/>

	<!-- I'm using JSON for debugging -->
	<inherits name="com.google.gwt.json.JSON" />

	<!-- Other module inherits -->
	<inherits name='com.google.gwt.maps.Maps' />

	<!-- Specify the app entry point class. -->
	<entry-point class='com.ifgov.client.IfGov' />

	<!-- Style sheets -->
<!-- 	<stylesheet src="css/style.css" /> -->

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>